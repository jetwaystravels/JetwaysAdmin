@model JetwaysAdmin.UI.ViewModel.MenuHeaddata
@{
    Layout = "~/Views/Shared/Admin.cshtml";
    var Data = Model;
}

<main id="main" class="main">
    <div class="pcoded-main-container">
        <div class="pcoded-wrapper">
            <!-- Page-header start -->
            <div class="page-header">
                <div class="page-block">
                    <div class="row align-items-center">
                        <div class="col-md-8">
                            <div class="page-header-title">
                                <h5 class="m-b-10"><a class="back" href="#" onclick="history.back();return false;" title="Back"><i class="fa fa-arrow-left mr-2" aria-hidden="true" style="vertical-align: text-top;"></i></a>Add New Group</h5>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <ul class="breadcrumb">
                                <li class="breadcrumb-item">
                                    <a href="index.html">Settings</a>
                                </li>
                                <li class="breadcrumb-item">
                                    <a href="#!">IATA Groups</a>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
            <!-- Page-header end -->
            <div class="pcoded-inner-content">
                <!-- Main-body start -->
                <div class="main-body">
                    <div class="page-wrapper">
                        <!-- Page-body start -->
                        <div class="page-body">
                            <!-- add new nupplier start -->
                            <div class="card">
                                <div class="card-block">
                                    <form class="form-material form-row">
                                        <div class="form-group form-primary col-md-3">
                                            <input type="text" name="group-Name" class="form-control" required>
                                            <span class="form-bar"></span>
                                            <label class="float-label">Group Name<span>*</span></label>
                                        </div>
                                        <div class="form-group form-primary col-md-3">
                                            <input type="text" name="Iata-code" class="form-control">
                                            <span class="form-bar"></span>
                                            <label class="float-label">IATA Code</label>
                                        </div>
                                        <div class="form-group form-primary col-md-3">
                                            <input type="text" name="group-code" class="form-control">
                                            <span class="form-bar"></span>
                                            <label class="float-label">Group Code</label>
                                        </div>
                                        <div class="form-group form-primary col-md-3">
                                            <input type="text" name="parent-company" class="form-control" required>
                                            <span class="form-bar"></span>
                                            <label class="float-label">Jetways Travels[JET001](Parent Company)<span>*</span></label>
                                        </div>
                                        <div class="form-group form-primary col-md-6">
                                            <input type="text" name="address-1" class="form-control">
                                            <span class="form-bar"></span>
                                            <label class="float-label">Address Line 1</label>
                                        </div>
                                        <div class="form-group form-primary col-md-6">
                                            <input type="text" name="address-2" class="form-control">
                                            <span class="form-bar"></span>
                                            <label class="float-label">Address Line 2<span>*</span></label>
                                        </div>
                                        <div class="form-group form-primary col-md-3">
                                            <select name="select" class="form-control fill">
                                                <option value="opt1">Country*</option>
                                                <option value="opt2">Type 2</option>
                                                <option value="opt3">Type 3</option>
                                                <option value="opt4">Type 4</option>
                                                <option value="opt5">Type 5</option>
                                                <option value="opt6">Type 6</option>
                                                <option value="opt7">Type 7</option>
                                                <option value="opt8">Type 8</option>
                                            </select>
                                            <span class="form-bar"></span>
                                        </div>
                                        <div class="form-group form-primary col-md-3">
                                            <select name="select" class="form-control fill">
                                                <option value="opt1">State*</option>
                                                <option value="opt2">Type 2</option>
                                                <option value="opt3">Type 3</option>
                                                <option value="opt4">Type 4</option>
                                                <option value="opt5">Type 5</option>
                                                <option value="opt6">Type 6</option>
                                                <option value="opt7">Type 7</option>
                                                <option value="opt8">Type 8</option>
                                            </select>
                                            <span class="form-bar"></span>
                                        </div>
                                        <div class="form-group form-primary col-md-3">
                                            <select name="select" class="form-control fill">
                                                <option value="opt1">City*</option>
                                                <option value="opt2">Type 2</option>
                                                <option value="opt3">Type 3</option>
                                                <option value="opt4">Type 4</option>
                                                <option value="opt5">Type 5</option>
                                                <option value="opt6">Type 6</option>
                                                <option value="opt7">Type 7</option>
                                                <option value="opt8">Type 8</option>
                                            </select>
                                            <span class="form-bar"></span>
                                        </div>
                                        <div class="form-group form-primary col-md-3">
                                            <input type="text" name="postal-code" class="form-control">
                                            <span class="form-bar"></span>
                                            <label class="float-label">Postal Code</label>
                                        </div>
                                        <div class="form-group form-primary col-md-3">
                                            <input type="text" name="group-email" class="form-control" required>
                                            <span class="form-bar"></span>
                                            <label class="float-label">Group Email<span>*</span></label>
                                        </div>
                                        <div class="form-group form-primary col-md-3">
                                            <input type="text" name="group-website" class="form-control">
                                            <span class="form-bar"></span>
                                            <label class="float-label">Group Website</label>
                                        </div>
                                        <div class="form-group form-primary col-md-1">
                                            <input type="text" name="country-code" class="form-control" required>
                                            <span class="form-bar"></span>
                                            <label class="float-label">GU ( +null)</label>
                                        </div>
                                        <div class="form-group form-primary col-md-2">
                                            <input type="number" name="phone-number" class="form-control">
                                            <span class="form-bar"></span>
                                            <label class="float-label">Phone Number<span>*</span></label>
                                        </div>
                                        <div class="form-group form-primary col-md-3">
                                            <input type="text" name="booking-reference" class="form-control" required>
                                            <span class="form-bar"></span>
                                            <label class="float-label">Booking Reference Prefix Valu<span>*</span></label>
                                        </div>
                                        <div class="form-group form-primary col-md-3">
                                            <select name="select" class="form-control fill">
                                                <option>Payment Option*</option>
                                                <option value="opt1">Cash/Deposit </option>
                                                <option value="opt2">Credit Card</option>
                                                <option value="opt3">Both</option>
                                            </select>
                                            <span class="form-bar"></span>
                                        </div>
                                        <div class="form-group form-primary col-md-3">
                                            <input type="text" name="Invoice-Prefix" class="form-control">
                                            <span class="form-bar"></span>
                                            <label class="float-label">Invoice Prefix Value</label>
                                        </div>
                                        <div class="form-group form-primary col-md-3">
                                            <input type="text" name="credit-note" class="form-control" required>
                                            <span class="form-bar"></span>
                                            <label class="float-label">Credit Note Prefix Value</label>
                                        </div>
                                        <div class="form-group form-primary col-md-3">
                                            <select name="select" class="form-control fill">
                                                <option>Select GDS & PCC for Passive PNR</option>
                                            </select>
                                            <span class="form-bar"></span>
                                        </div>
                                        <div class="form-group form-primary col-md-3">
                                            <select name="select" class="form-control fill">
                                                <option>Select Default Visa Team</option>
                                                <option>Hotels Group</option>
                                                <option>Pune Consultant Group</option>
                                                <option>Delhi Consultant Group</option>
                                            </select>
                                            <span class="form-bar"></span>
                                        </div>
                                        <div class="form-group form-primary col-md-3">
                                            <input type="text" name="Arn-number" class="form-control">
                                            <span class="form-bar"></span>
                                            <label class="float-label">ARN Number</label>
                                        </div>
                                        <div class="form-group form-primary col-md-3">
                                            <input type="text" name="validity-period" class="form-control">
                                            <span class="form-bar"></span>
                                            <label class="float-label">Validity Period</label>
                                        </div>

                                        <div class="form-group form-primary col-md-7 m-l-5">
                                            <button class="btn btn-primary waves-effect waves-light">Add New</button>
                                            <button class="btn btn-danger waves-effect waves-light">Cancel</button>
                                        </div>
                                    </form>
                                </div>
                            </div>
                            <!-- add new nupplier end -->
                        </div>
                        <!-- Page-body end -->
                    </div>
                    <div id="styleSelector"> </div>
                </div>
            </div>
        </div>
    </div>
</main>
