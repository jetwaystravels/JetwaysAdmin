@model JetwaysAdmin.UI.ViewModel.MenuHeaddata
@{
    Layout = "~/Views/Shared/Admin.cshtml";
    var Data = Model;
}

<main id="main" class="main">
    <div class="pcoded-main-container">
        <div class="pcoded-wrapper">
            <!-- Page-header start -->
            <div class="page-header">
                <div class="page-block">
                    <div class="row align-items-center">
                        <div class="col-md-8">
                            <div class="page-header-title">
                                <h5 class="m-b-10">IATA Groups</h5>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <ul class="breadcrumb">
                                <li class="breadcrumb-item">
                                    <a href="index.html">Settings</a>
                                </li>
                                <li class="breadcrumb-item">
                                    <a href="#!">Manage Suppliers</a>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
            <!-- Page-header end -->
            <div class="pcoded-inner-content">
                <!-- Main-body start -->
                <div class="main-body">
                    <div class="page-wrapper">
                        <!-- Page-body start -->
                        <div class="page-body">
                            <div class="d-flex justify-content-between m-b-10">
                                <form class="form-material">
                                    <div class="form-group form-primary" style="width:280px;">
                                        <input type="text" name="footer-email" class="form-control">
                                        <span class="form-bar"></span>
                                        <label class="float-label"><i class="fa fa-search m-r-10"></i>Search</label>
                                    </div>
                                </form>
                                <div><a asp-controller="IATAGroup" asp-action="AddIATAGroup"><button class="btn sml-btn-danger waves-effect waves-light">+ Add New</button></a></div>
                            </div>
                            <!-- IATA groups start -->
                            <div class="card">
                                <div class="card-block">
                                    <div class="card-block table-border-style">
                                        <div class="table-responsive">
                                            <table class="table">
                                                <thead>
                                                    <tr>
                                                        <th><button class="srt-btn" type="button" aria-label="Group Name">Group Name</button></th>
                                                        <th><button class="srt-btn" type="button" aria-label="Group Code">Group Code</button></th>
                                                        <th><button class="srt-btn" type="button" aria-label="City,State,Country">City,State,Country</button></th>
                                                        <th><button class="srt-btn" type="button" aria-label="Address">Address</button></th>
                                                        <th><button class="srt-btn" type="button" aria-label="Status">Status</button></th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <tr style="cursor:pointer;">
                                                        <td onclick="location.href='iatadetails.html';">Jetways Delhi </td>
                                                        <td onclick="location.href='iatadetails.html';"> DEL </td>
                                                        <td onclick="location.href='iatadetails.html';"> India, Delhi, New Delhi </td>
                                                        <td onclick="location.href='iatadetails.html';"><span class="letter-length">CONNAUGHT PLACE ,NEW DELHI, UGF 5, INDRA PRAKASH BUILDING 21, B. K. ROAD</span></td>
                                                        <td>
                                                            <label class="switch">
                                                                <input type="checkbox">
                                                                <span class="slider round"></span>
                                                            </label>
                                                        </td>
                                                    </tr>
                                                    <tr style="cursor:pointer;">
                                                        <td onclick="location.href='iatadetails.html';"> Jetways Gurgaon  </td>
                                                        <td onclick="location.href='iatadetails.html';"></td>
                                                        <td onclick="location.href='iatadetails.html';"> India, Haryana, Gurgaon  </td>
                                                        <td onclick="location.href='iatadetails.html';"><span class="letter-length"></span></td>
                                                        <td>
                                                            <label class="switch">
                                                                <input type="checkbox">
                                                                <span class="slider round"></span>
                                                            </label>
                                                        </td>
                                                    </tr>
                                                    <tr style="cursor:pointer;">
                                                        <td onclick="location.href='iatadetails.html';"> Jetways Pune </td>
                                                        <td onclick="location.href='iatadetails.html';"> PNQ </td>
                                                        <td onclick="location.href='iatadetails.html';"> India, Delhi, New Delhi </td>
                                                        <td onclick="location.href='iatadetails.html';"><span class="letter-length">CONNAUGHT PLACE ,NEW DELHI, UGF 5, INDRA PRAKASH BUILDING 21, B. K. ROAD</span></td>
                                                        <td>
                                                            <label class="switch">
                                                                <input type="checkbox">
                                                                <span class="slider round"></span>
                                                            </label>
                                                        </td>
                                                    </tr>
                                                    <tr style="cursor:pointer;">
                                                        <td onclick="location.href='iatadetails.html';"> Jetways Mumbai</td>
                                                        <td onclick="location.href='iatadetails.html';"> India, Maharashtra, Mumbai </td>
                                                        <td onclick="location.href='iatadetails.html';"> India, Delhi, New Delhi </td>
                                                        <td onclick="location.href='iatadetails.html';"><span class="letter-length"></span></td>
                                                        <td onclick="location.href='iatadetails.html';">
                                                            <label class="switch">
                                                                <input type="checkbox">
                                                                <span class="slider round"></span>
                                                            </label>
                                                        </td>
                                                    </tr>
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="d-flex justify-content-between pagination-section">
                                <span class="pagination-info">
                                    Showing 1 to 10 of 100
                                </span>
                                <div class="float-right pagination">
                                    <span class="m-t-5 m-r-5">Items per page:</span>
                                    <select class="form-select form-select-sm">
                                        <option selected>5</option>
                                        <option value="1">10</option>
                                        <option value="2">25</option>
                                        <option value="3">100</option>
                                    </select>
                                    <ul class="pagination">
                                        <li class="page-item page-pre"><a class="page-link" aria-label="previous page" href="javascript:void(0)">‹</a></li>
                                        <li class="page-item active"><a class="page-link" aria-label="to page 1" href="javascript:void(0)">1</a></li>
                                        <li class="page-item"><a class="page-link" aria-label="to page 2" href="javascript:void(0)">2</a></li>
                                        <li class="page-item"><a class="page-link" aria-label="to page 3" href="javascript:void(0)">3</a></li>
                                        <li class="page-item"><a class="page-link" aria-label="to page 4" href="javascript:void(0)">4</a></li>
                                        <li class="page-item"><a class="page-link" aria-label="to page 5" href="javascript:void(0)">5</a></li>
                                        <li class="page-item page-last-separator disabled"><a class="page-link" aria-label="" href="javascript:void(0)">...</a></li>
                                        <li class="page-item"><a class="page-link" aria-label="to page 80" href="javascript:void(0)">80</a></li>
                                        <li class="page-item page-next"><a class="page-link" aria-label="next page" href="javascript:void(0)">›</a></li>
                                    </ul>
                                </div>
                            </div>
                            <!-- IATA groups end -->
                        </div>
                        <!-- Page-body end -->
                    </div>
                </div>
            </div>
        </div>
    </div>
</main>
