@using JetwaysAdmin.UI.ViewModel
@model List<MenuHeaddata>

@{
    Layout = "~/Views/Shared/Admin.cshtml";
}



<main id="main" class="main">
    <!-- Pre-loader start -->
    <div class="theme-loader">
        <div class="loader-track">
            <div class="preloader-wrapper">
                <div class="spinner-layer spinner-blue">
                    <div class="circle-clipper left">
                        <div class="circle"></div>
                    </div>
                    <div class="gap-patch">
                        <div class="circle"></div>
                    </div>
                    <div class="circle-clipper right">
                        <div class="circle"></div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Pre-loader end -->

    <div class="pcoded-main-container">
        <div class="pcoded-wrapper">
            <div>
                <!-- Page-header start -->
                <div class="page-header">
                    <div class="page-block">
                        <div class="row align-items-center">
                            <div class="col-md-8">
                                <div class="page-header-title">
                                    <h5 class="m-b-10"><a href="customers.html"><i class="fa fa-arrow-left mr-2" aria-hidden="true" style="vertical-align: text-top;"></i></a>Add New Customer</h5>
                                </div>
                            </div>
                            <div class="col-md-4">
                                <ul class="breadcrumb">
                                    <li class="breadcrumb-item">
                                        <a href="index.html">Settings</a>
                                    </li>
                                    <li class="breadcrumb-item">
                                        Manage Customers
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- Page-header end -->
                <div class="pcoded-inner-content">
                    <!-- Main-body start -->
                    <div class="main-body">
                        <div class="page-wrapper">
                            <!-- Page-body start -->
                            <div class="page-body">
                                <div class="card">
                                    <div class="card-block">
                                        <!--  suppeir info Start -->
                                        <form class="form-material form-row">
                                            <div class="form-group form-primary col-md-3">
                                                <input type="text" name="legal-entity" class="form-control" required>
                                                <span class="form-bar"></span>
                                                <label class="float-label">Legal Entity Name<span>*</span></label>
                                            </div>
                                            <div class="form-group form-primary col-md-3">
                                                <input type="text" name="legal-entity-code" class="form-control">
                                                <span class="form-bar"></span>
                                                <label class="float-label">Legal Entity Code</label>
                                            </div>
                                            <div class="form-group form-primary col-md-3">
                                                <select name="select" class="form-control fill">
                                                    <option value="opt1">Assign IATA Group</option>
                                                    <option value="opt2">Jetways Delhi</option>
                                                    <option value="opt3">Jetways Gurgaon</option>
                                                    <option value="opt4">Jetways Pune</option>
                                                    <option value="opt5">Jetways Mombai</option>
                                                </select>
                                                <span class="form-bar"></span>
                                            </div>
                                            <div class="form-group form-primary col-md-3">
                                                <input type="text" name="corporate-accounts" class="form-control" required>
                                                <span class="form-bar"></span>
                                                <label class="float-label">Corporate Accounts Code</label>
                                            </div>
                                            <div class="form-group form-primary col-md-3">
                                                <input type="text" name="first-name" class="form-control" required>
                                                <span class="form-bar"></span>
                                                <label class="float-label">First Name</label>
                                            </div>
                                            <div class="form-group form-primary col-md-3">
                                                <input type="text" name="last-name" class="form-control">
                                                <span class="form-bar"></span>
                                                <label class="float-label">Last Name</label>
                                            </div>
                                            <div class="form-group form-primary col-md-3">
                                                <input type="text" name="business-email-id" class="form-control">
                                                <span class="form-bar"></span>
                                                <label class="float-label">Business Email ID</label>
                                            </div>

                                            <div class="form-group form-primary col-md-3">
                                                <input type="password" name="password" class="form-control">
                                                <span class="form-bar"></span>
                                                <label class="float-label">Password</label>
                                            </div>

                                            <div class="form-group form-primary col-md-1">
                                                <select class="select">
                                                    <option value="">IN(+91)</option>
                                                    <option value="AF">AF (+93)</option>
                                                    <option value="AL">AL (+355)</option>
                                                    <option value="DZ">DZ (+213)</option>
                                                    <option value="AO">AO (+244)</option>
                                                    <option value="AR">AR (+54)</option>
                                                    <option value="AM">AM (+374)</option>
                                                    <option value="AW">AW (+297)</option>
                                                    <option value="AU">AU (+61)</option>
                                                    <option value="AZ">AZ (+994)</option>
                                                </select>
                                            </div>

                                            <div class="form-group form-primary col-md-2">
                                                <input type="number" name="phone-number" class="form-control">
                                                <span class="form-bar"></span>
                                                <label class="float-label"></label>
                                            </div>

                                            <div class="form-group form-primary col-md-3">
                                                <select name="select" class="form-control fill">
                                                    <option value="opt1">Gender</option>
                                                    <option value="opt2">Male</option>
                                                    <option value="opt3">Female</option>
                                                </select>
                                                <span class="form-bar"></span>
                                            </div>

                                            <div class="form-group form-primary col-md-3">
                                                <input type="text" name="address" class="form-control" required>
                                                <span class="form-bar"></span>
                                                <label class="float-label">Registered Address Line 1<span>*</span></label>
                                            </div>
                                            <div class="form-group form-primary col-md-3">
                                                <input type="text" name="address" class="form-control" required>
                                                <span class="form-bar"></span>
                                                <label class="float-label">Registered Address Line 2<span>*</span></label>
                                            </div>

                                            <div class="form-group form-primary col-md-3">
                                                <select class="select">
                                                    <option value="">Country*</option>
                                                    <option value="AF">Abkhazia</option>
                                                    <option value="AL">Afghanistan</option>
                                                    <option value="DZ">Akrotiri and Dhekelia</option>
                                                    <option value="AO">Aland Island</option>
                                                    <option value="AR">Albania</option>
                                                    <option value="AM">Algeria</option>
                                                    <option value="AW">American Samoa</option>
                                                    <option value="AU">Andorra</option>
                                                    <option value="AZ">Angola</option>
                                                </select>
                                            </div>
                                            <div class="form-group form-primary col-md-3">
                                                <select class="select">
                                                    <option value="">State*</option>
                                                </select>
                                            </div>
                                            <div class="form-group form-primary col-md-3">
                                                <select class="select">
                                                    <option value="">City*</option>
                                                </select>
                                            </div>
                                            <div class="form-group form-primary col-md-3">
                                                <input type="text" name="creat-type" class="form-control">
                                                <span class="form-bar"></span>
                                                <label class="float-label">Postal Code<span>*</span></label>
                                            </div>

                                            <div class="form-group form-primary col-md-3">
                                                <select name="select" class="form-control fill">
                                                    <option value="opt1">Account Type*</option>
                                                    <option value="opt2">Business</option>
                                                    <option value="opt3">Personal</option>
                                                </select>
                                                <span class="form-bar"></span>
                                            </div>

                                            <div class="form-group form-primary col-md-3">
                                                <input type="text" name="creat-type" class="form-control">
                                                <span class="form-bar"></span>
                                                <label class="float-label">Integration Ref. Number</label>
                                            </div>
                                            <div class="form-group form-primary col-md-3">
                                                <select class="select">
                                                    <option value="">Customer Base Currency* (₹ INR)</option>
                                                    <option value="AF">Afghanistan</option>
                                                    <option value="AL">Albania</option>
                                                    <option value="DZ">Algeria</option>
                                                    <option value="AO">Angola</option>
                                                    <option value="AR">Argentina</option>
                                                    <option value="AM">Armenia</option>
                                                    <option value="AW">Aruba</option>
                                                    <option value="AU">Australia</option>
                                                    <option value="AZ">Austria</option>
                                                </select>
                                            </div>
                                            <div class="form-group form-primary col-md-3">
                                                <select class="select">
                                                    <option value="">Customer Base Country*</option>
                                                    <option value="AF">Afghanistan</option>
                                                    <option value="AL">Albania</option>
                                                    <option value="DZ">Algeria</option>
                                                    <option value="AO">Angola</option>
                                                    <option value="AR">Argentina</option>
                                                    <option value="AM">Armenia</option>
                                                    <option value="AW">Aruba</option>
                                                    <option value="AU">Australia</option>
                                                    <option value="AZ">Austria</option>
                                                </select>
                                            </div>

                                            <div class="form-group form-primary col-md-3">
                                                <input class="form-control date-main" type="text" placeholder="Account Activation Date">
                                                <span class="form-bar"></span>
                                            </div>

                                            <div class="form-group form-primary col-md-3">
                                                <input class="form-control date-main" type="text" placeholder="Account De-activation Date">
                                                <span class="form-bar"></span>
                                            </div>
                                            <div class="form-group form-primary col-md-3 m-t-15">
                                                <strong>GST Applicable on Management Fee</strong>
                                                <label class="switch m-l-5">
                                                    <input type="checkbox">
                                                    <span class="slider round"></span>
                                                </label>
                                            </div>

                                            <div class="form-group form-primary col-md-3 m-t-15">
                                                <strong>Pass GST Details to the Airline</strong>
                                                <label class="switch m-l-5">
                                                    <input type="checkbox">
                                                    <span class="slider round"></span>
                                                </label>
                                            </div>
                                            <div class="form-check form-check-inline col-md-12 m-b-20">
                                                <label class="radio_lb">
                                                    <input class="form-check-input" type="radio" name="redio-btn" value="option1" checked>
                                                    Create new GDS profile
                                                </label>
                                                <label class="radio_lb">

                                                    <input class="form-check-input" type="radio" name="redio-btn" value="option1">
                                                    Update existing customer profile
                                                </label>
                                            </div>

                                            <div class="form-group form-primary col-md-4">
                                                <input type="file" name="upload-logo" class="form-control" placeholder="Logo">
                                                <span class="form-bar"></span>
                                                <span style="font-size:11px;font-weight:700;">Upload logo Allowed formats: .JPG / .PNG, Maximum file size: 1mb, Resolution 200px x 90px</span>
                                            </div>
                                            <div class="form-group form-primary col-md-12">
                                                <button class="btn btn-primary waves-effect waves-light disabled">Update</button>
                                                <button class="btn btn-danger waves-effect waves-light">Cancel</button>
                                            </div>
                                        </form>
                                    </div>
                                </div>
                                <!--  suppliers info end -->
                            </div>
                        </div>
                    </div>
                    <!-- Page-body end -->
                </div>
            </div>
        </div>
    </div>
</main>

<script>
    $(function () {
        $(".select").select2();
    });
</script>
<!-- select with search js End -->
<!-- datepicker start -->
<script>
    $(function () {
        $(".date-main").datepicker({
            changeMonth: true,
            changeYear: true
        });
    });
</script>
<!-- datepicker End -->

<script>
    $(".add").click(function () {
        $(this).closest('.content-one').append('<div class="row p-l-p DomainsEmail"><div class="col-md-12"><input class="form-control" type="text" placeholder="Please enter domain name"><span class="remove"><i class="add fa fa-minus-circle m-t-15" aria-hidden="true" style="color: #ff1515; cursor:pointer; position: absolute; right: 22px;"></i></span></div></div>');
    });
</script>

<script>
    $(".add").click(function () {
        $(this).closest('.content-two').append('<div class="row p-l-p DomainsEmail"><div class="col-md-12"><input class="form-control" type="text" placeholder="Please add approver email(s) in comma seperated"><span class="remove"><i class="add fa fa-minus-circle m-t-15" aria-hidden="true" style="color: #ff1515; cursor:pointer; position: absolute; right: 22px;"></i></span></div></div>');
    });
</script>

<script>
    $("body").on('click', '.remove', function () {
        $(this).parents('.DomainsEmail').remove();
    });
</script>