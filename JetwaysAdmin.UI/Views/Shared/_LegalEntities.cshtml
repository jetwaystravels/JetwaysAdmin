                                            <a href="auth-normal-sign-in.html">
                                                <strong style="font-weight:600;"><i class="fa fa-sign-out" aria-hidden="true"></i> Logout</strong>
                                            </a>
                                        </li>
                                    </ul>
                                </li>
                            </ul>
                        </div>
